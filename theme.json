	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "currentColor",
					"radius": "0px",
					"style": "var(--wp--custom--shaded-style--border--style)",
					"width": "var(--wp--custom--shaded-style--border--width)"
				},
				"color": {
					"background": "var(--wp--preset--color--background)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": {
						"top": "15px",
						"right": "24px",
						"bottom": "15px",
						"left": "24px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--heading)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/calendar": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/cover": {
				"color": {
					"text": "#ffffff"
				},
				"spacing": {
					"padding": "var( --wp--custom--gutter )"
				}
			},
			"core/file": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "1em",
						"top": "0px"
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"bottom": "0px",
						"left": "0px",
						"right": "0px",
						"top": "0px"
					}
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/site-title": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--normal)"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "0px"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)",
			"link": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--letter-spacing--gigantic)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"letterSpacing": "var(--wp--custom--letter-spacing--heading)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)",
